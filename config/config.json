{
    "app_name": "gitframe",
    "author": "Gabriel Auger",
    "debug": false,
    "deps": [
        {
            "app": "git",
            "cmd": "git"
        },
        {
            "app": "scp",
            "cmd": "scp"
        },
        {
            "app": "ssh",
            "cmd": "ssh"
        },
        {
            "app": "ping",
            "cmd": "ping"
        },
        {
            "app": "openssh-server",
            "cmd": "ssh-keygen"
        },
        {
            "app": "openssh-client",
            "cmd": "ssh"
        }
    ],
    "diren_scripts": "scripts",
    "diren_src": "src",
    "filen_hotfix_history": "hotfix-history.json",
    "filen_version": "version.txt",
    "filer_bump_release_version": "bump_release_version",
    "filer_deploy_release": "deploy_release",
    "license": "MIT",
    "processor": {
        "clean_after": false,
        "deps": [
            {
                "app": "bash",
                "cmd": "bash"
            },
            {
                "app": "xdotool",
                "cmd": "xdotool"
            },
            {
                "app": "wmctrl",
                "cmd": "wmctrl"
            },
            {
                "app": "screen",
                "cmd": "screen"
            },
            {
                "app": "tput",
                "cmd": "tput"
            },
            {
                "app": "xterm",
                "cmd": "xterm"
            }
        ],
        "task": {
            "diren": "test",
            "direpa_root": "/tmp/test-gf",
            "remote": {
                "diren_root": "repository",
                "domain": "localhost",
                "ip": "127.0.0.1",
                "user_git": "noa"
            }
        },
        "diren_logs": "logs",
        "filen_blank_page": "print_blank_page.sh",
        "filen_conf": "processor_config.json",
        "filen_read_log_bashrc": "read_log_bashrc",
        "filen_screen_log": "processor_screen_log.0",
        "filen_screenrc": "screenrc",
        "filen_xterm": "launch_xterm.sh",
        "filen_tmux": "launch_tmux.sh",
        "read_log_window_title": "#Error_Logs#",
        "txt_screen_cmd_error": "++ cmd error ++",
        "txt_screen_log_eof": "++ End Unit ++",
        "waiting_time_between_cmds": 20
    },
    "version": "1.0.0-beta-1541130477"
}